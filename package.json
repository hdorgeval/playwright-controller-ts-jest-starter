{
  "name": "playwright-fluent-ts-jest-starter",
  "version": "1.0.0",
  "description": "Starter project for e2e tests with playwright-fluent and ts jest",
  "main": "index.js",
  "scripts": {
    "build": "rimraf build && npm run format && npm run lint && tsc",
    "check-format": "prettier --list-different \"src/**/*.ts\" ",
    "format": "prettier --write \"src/**/*.ts\" ",
    "jest-init": "jest --init",
    "lint": "eslint . --ext .js,.ts --format visualstudio --no-color --max-warnings 10 --report-unused-disable-directives",
    "lint-fix": "npm run lint -- --fix",
    "test": "jest --config=jest.config.js --runInBand",
    "tsc-init": "tsc --init",
    "tsc": " tsc --listEmittedFiles",
    "install-playwright-with-no-browser-download": "env-cmd npm i playwright --save"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hdorgeval/playwright-fluent-ts-jest-starter.git"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "keywords": [
    "playwright",
    "playwright-fluent",
    "fluent-api",
    "e2e",
    "test-automation",
    "test-runner"
  ],
  "author": "Henri d'Orgeval",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hdorgeval/playwright-fluent-ts-jest-starter/issues"
  },
  "homepage": "https://github.com/hdorgeval/playwright-fluent-ts-jest-starter#readme",
  "dependencies": {
    "playwright": "1.8.0",
    "playwright-fluent": "1.18.0"
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.22",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "@typescript-eslint/parser": "4.14.0",
    "env-cmd": "10.1.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "typescript": "3.9.7"
  }
}
