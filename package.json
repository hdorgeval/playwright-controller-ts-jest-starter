{
  "name": "playwright-fluent-ts-jest-starter",
  "version": "1.0.0",
  "description": "Starter project for e2e tests with playwright-fluent and ts jest",
  "main": "index.js",
  "scripts": {
    "build": "rimraf build && npm run format && npm run lint && tsc",
    "check-format": "prettier --list-different \"src/**/*.ts\" ",
    "format": "prettier --write \"src/**/*.ts\" ",
    "jest-init": "jest --init",
    "lint": "eslint . --ext .js,.ts --format visualstudio --no-color --max-warnings 10 --report-unused-disable-directives",
    "lint-fix": "npm run lint -- --fix",
    "test": "jest --config=jest.config.js --runInBand",
    "tsc-init": "tsc --init",
    "tsc": " tsc --listEmittedFiles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hdorgeval/playwright-fluent-ts-jest-starter.git"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "keywords": [
    "playwright",
    "playwright-fluent",
    "fluent-api",
    "e2e",
    "test-automation",
    "test-runner"
  ],
  "author": "Henri d'Orgeval",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hdorgeval/playwright-fluent-ts-jest-starter/issues"
  },
  "homepage": "https://github.com/hdorgeval/playwright-fluent-ts-jest-starter#readme",
  "dependencies": {
    "playwright-fluent": "0.24.1"
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/jest": "25.1.2",
    "@types/node": "13.7.2",
    "@typescript-eslint/eslint-plugin": "2.20.0",
    "@typescript-eslint/parser": "2.20.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.2",
    "ts-jest": "25.2.1",
    "ts-node": "8.6.2",
    "typescript": "3.7.5"
  }
}
